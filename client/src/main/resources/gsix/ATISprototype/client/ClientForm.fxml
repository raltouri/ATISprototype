<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gsix.ATISprototype.client.ClientFormController" style="-fx-background-color: #f0f0f0;">
   <children>
      <Label layoutX="50.0" layoutY="122.0" text="Server IP:" />
      <Label layoutX="50.0" layoutY="227.0" text="Server Port:" />
      <TextField fx:id="ip_TF" layoutX="150.0" layoutY="112.0" prefHeight="39.0" prefWidth="400.0" promptText="Enter IP Address" />
      <TextField fx:id="port_TF" layoutX="150.0" layoutY="217.0" prefHeight="39.0" prefWidth="400.0" promptText="Enter Port Number" />
      <Button fx:id="connectToServer_Btn" layoutX="250.0" layoutY="292.0" mnemonicParsing="false" onAction="#ConnectToServer" text="Connect To Server" style="-fx-background-color: #4CAF50; -fx-text-fill: white;" />
   </children>
</AnchorPane>

